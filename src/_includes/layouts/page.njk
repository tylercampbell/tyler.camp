<!doctype html>
<html lang="en"> 
  <head>
    <meta charset="utf-8">
    <link rel="icon" href="/favicon.ico" sizes="any">
    <link rel="icon" href="/favicon.svg" type="image/svg+xml">
    <link rel="apple-touch-icon" href="/apple-touch-icon.png">
    
    <meta name="theme-color" content="{{ bgcolor }}">

    {% metagen
      title="Tyler Campbell",
      name="Tyler Campbell",
      desc="Tyler Campbell is a Creative Director in the beautiful state of Georgia.",
      url="https://www.tyler.camp" + page.url,
      img="https://www.tyler.camp/ogimage-ty-memoji-01.jpg",
      img_alt="Tyler Campbell",
      twitter_handle="tylerscramble",
      generator="eleventy",
      twitter_card_type="summary_large_image"
    %}

    {% if scripts %}
      <script type="module">
        {% set js %}{% include scripts %}{% endset %}
        {{ js | jsmin | safe }}
      </script>
    {% endif %}

    {% if styles %}
      <style>
        {% set css %}{% include styles %}{% endset %}
        {{ css | cssmin }}
      </style>
    {% endif %}

    <link href="/assets/styles.css" rel="stylesheet"/>
    <script src="/assets/scripts.js" defer></script>
  
  </head>

  <body style="background-color: {{ bgcolor }}">
    {# {% include "nav-prev-next.njk" %} #}
    {{ content | safe }}
  </body>

</html>