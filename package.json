{
  "scripts": {
    "clean": "rimraf _site",
    "serve:postcss": "postcss ./src/_includes/assets/styles.css -o ./src/_includes/assets/_compiled/compiled.css --watch",
    "serve:eleventy": "eleventy --serve --incremental",
    "dev": "cross-env NODE_ENV=development npm-run-all clean build:postcss -p serve:*",
    "build:postcss": "postcss ./src/_includes/assets/styles.css -o ./src/_includes/assets/_compiled/compiled.css",
    "build:eleventy": "eleventy",
    "build": "cross-env NODE_ENV=production npm-run-all clean -s build:*"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.20",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "eleventy-plugin-metagen": "^1.7.11",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-lightningcss": "^0.6.0",
    "rimraf": "^3.0.2"
  }
}
